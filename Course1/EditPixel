//https://www.coursera.org/learn/duke-programming-web/supplement/XwS1Y/try-it-using-for-loops
//CHange the image colors pixel by pixel.
var img = new SimpleImage(20,20);
print(img);

for(var pixel of img.values()){
    
    pixel.setGreen(255);
    pixel.setBlue(0);
    pixel.setRed(255);
}

print(img);
